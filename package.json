{
  "name": "flowcraft",
  "version": "1.0.0",
  "description": "Privacy-first HTTP manipulation toolkit for developers",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,json,css,md}\"",
    "type-check": "tsc --noEmit",
    "package": "npm run build && node scripts/package.js",
    "prepare": "husky install"
  },
  "dependencies": {
    "clsx": "^2.0.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-hook-form": "^7.69.0",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.1",
    "@tailwindcss/postcss": "^4.1.18",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.5.1",
    "@types/chrome": "^0.1.32",
    "@types/jest": "^30.0.0",
    "@types/node": "^25.0.3",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@types/webextension-polyfill": "^0.12.4",
    "@typescript-eslint/eslint-plugin": "^8.50.0",
    "@typescript-eslint/parser": "^8.50.0",
    "@vitejs/plugin-react": "^5.1.2",
    "autoprefixer": "^10.4.16",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^7.0.1",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "postcss": "^8.4.32",
    "prettier": "^3.1.1",
    "tailwindcss": "^4.1.18",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3",
    "vite": "^7.3.0",
    "vite-plugin-web-extension": "^4.5.0",
    "webextension-polyfill": "^0.12.0"
  },
  "keywords": [
    "chrome-extension",
    "http",
    "developer-tools",
    "privacy",
    "open-source",
    "request-interception",
    "header-modification",
    "api-mocking"
  ],
  "author": "FlowCraft Contributors",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/imerljak/flow-craft"
  }
}
